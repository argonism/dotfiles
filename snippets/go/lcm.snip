

snippet lcm
  func gcd (a, b float64) float64 {
    if a > b {
      return _gcd(a, b)
    }
    return _gcd(b, a)
  }


  func _gcd (a, b float64) float64 {
    if b != 0 {
      return _gcd(b, math.Mod(a, b))
    }
    return a
  }


  func lcm (a, b float64) float64 {
    return (a * b) / gcd(a, b)
  }

